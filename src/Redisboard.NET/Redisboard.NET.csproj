<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <LangVersion>default</LangVersion>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="8.0.0" />
        <PackageReference Include="StackExchange.Redis" Version="2.7.33" />
        <PackageReference Include="System.Text.Json" Version="8.0.3" />
    </ItemGroup>

    <ItemGroup>
        <InternalsVisibleTo Include="Redisboard.NET.Tests" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="Scripts\get_player_ids_by_range_competition_rank.lua" />
      <EmbeddedResource Include="Scripts\get_player_ids_by_range_competition_rank.lua">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </EmbeddedResource>
      <None Remove="Scripts\get_player_ids_by_range_dense_rank.lua" />
      <EmbeddedResource Include="Scripts\get_player_ids_by_range_dense_rank.lua">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </EmbeddedResource>
    </ItemGroup>

</Project>
